# .env.example - plantilla local (NO comitear tu .env con credenciales reales)
# Variables de entorno usadas por application-cloud.properties.example

# URL JDBC completa
# Ejemplo: jdbc:postgresql://containers-us-west-123.railway.app:5432/dbname
SPRING_DATASOURCE_URL=
# Usuario/Password de la BD (provistos por el proveedor)
SPRING_DATASOURCE_USERNAME=
SPRING_DATASOURCE_PASSWORD=

# OPCIONAL: Overwrite the spring profile
SPRING_PROFILES_ACTIVE=cloud
